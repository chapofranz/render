<style>
    .container {
        margin-top: 50px;
    }
    .panel {
        border-radius: 0;
    }
    .panel-heading {
        background-color: #428bca;
        color: white;
        border-radius: 0;
    }
    .panel-body {
        padding: 20px;
    }
    .dashboard-icons {
        font-size: 4em;
        margin-bottom: 10px;
    }
    .dashboard-link {
        color: #428bca;
    }
</style>
<title> Dashboard </title>
<div class="container-fluid">
    <div class="row">
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">User</h3>
                    <%= benutzer.vorname %>
                    <%= benutzer.nachname %>
                    <%= benutzer.studiengang.name %>
                
                </div>
                <div class="panel-body">
                    <a href="#" class="dashboard-link">
                        <span class="glyphicon glyphicon-user dashboard-icons"></span>
                        <span class="dashboard-text">View Users</span>
                    </a>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Projects</h3>
                </div>
                <div class="panel-body">
                    <a href="#" class="dashboard-link">
                        <span class="glyphicon glyphicon-folder-open dashboard-icons"></span>
                        <span class="dashboard-text">View Projects</span>
                    </a>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Settings</h3>
                </div>
                <div class="panel-body">
                    <a href="#" class="dashboard-link">
                        <span class="glyphicon glyphicon-cog dashboard-icons"></span>
                        <span class="dashboard-text">View Settings</span>
                    </a>
                </div>
            </div>
        </div>
    </div>
</div>