<div class="container">
    <h1>Studiengang hinzufügen</h1>
    <form action="/studiengang/create" method="POST">
        <div class="form-group">
            <label for="name">Name</label>
            <input type="text" class="form-control" id="name" name="name" required>
        </div>
        <div class="form-group">
            <label for="kuerzel">Kürzel</label>
            <input type="text" class="form-control" id="kuerzel" name="kuerzel" required>
        </div>
        <button type="submit" class="btn btn-primary">Studiengang hinzufügen</button>
    </form>
</div>
