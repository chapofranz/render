<div class="main-container container mt-5">
    <div class="row justify-content-center">
        <div class="col-12">
            <h1 class="text-center mb-4">Dozenten</h1>
        </div>
        <% for (let i=0; i < dozenten.length; i++) { %>
            <div class="col-12 col-sm-10 col-md-8 col-lg-6 mb-4">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">
                            <%= dozenten[i].vorname %>
                                <%= dozenten[i].nachname %>
                        </h5>
                        <p class="card-text">
                            <strong>Email:</strong>
                            <%= dozenten[i].email %> <br>
                                <strong>Telefonnummer:</strong>
                                <%= dozenten[i].telefonnummer %>
                        </p>
                    </div>
                </div>
            </div>
            <% } %>
    </div>
</div>